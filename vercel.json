{
  "builds": [
    {
      "src": "jun/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "chartly/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "template/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/jun/assets/(.*)",
      "dest": "/jun/assets/$1"
    },
    {
      "src": "/jun/(.*)\\.(?:js|css|map)$",
      "dest": "/jun/$1"
    },
    {
      "src": "/jun/(.*)",
      "dest": "/jun/index.html"
    },
    {
      "src": "/chartly/assets/(.*)",
      "dest": "/chartly/assets/$1"
    },
    {
      "src": "/chartly/(.*)\\.(?:js|css|map)$",
      "dest": "/chartly/$1"
    },
    {
      "src": "/chartly/(.*)",
      "dest": "/chartly/index.html"
    }
  ]
}
